.inputfile{
width: 0.1px;
height: 0.1px;
opacity: 0;
overflow: hidden;
position: absolute;
z-index: -1;
}
.inputfile + label {
   /* max-width: 80%;
     font-size: 1.25rem; */
    font-weight: 700;
    text-overflow: ellipsis;
    white-space: nowrap;
    cursor: pointer;
    display: inline-block;
    overflow: hidden;
    /* padding: 0.625rem 1.25rem; */
    border: 1px solid #b3adad;
    vertical-align: bottom;
    background-color: #fcfafa;
}
.inputfile + label span {
    width: 327px;
    display: inline-block;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    vertical-align: top;
}
strong.iconUpload svg {
    width: 1em;
    height: 1em;
    vertical-align: middle;
    fill: currentColor;
    margin-top: -0.25em;
    margin-right: 0.25em;
}
.inputfile + label span, .inputfile + label strong {
    padding: 0.625rem 1.25rem;
}
.inputfile + label strong {
    height: 100%;
    color: #f1e5e6;
    background-color: #0095fb;
    display: inline-block;
}
.inputfile + label svg {
    width: 1em;
    height: 1em;
    vertical-align: middle;
    fill: currentColor;
    margin-top: -0.25em;
    margin-right: 0.25em;
}
.inputfile + label {
    color: #403f3f;
}
.iconUpload{
	height: 45px;
    color: #f1e5e6;
    background-color: #767676 !important;
    display: inline-block;
    margin-left: 0px;
    padding: 8px;
    vertical-align: bottom;
}
input.text {
    border: 1px solid #b3adad !important;
    height: 34px !important;
}
.csfWidgets .textinput .button{
    float:none !important;
}
input#uploadStartDateId-field-textInput,input#modal-textInput {
    width: 329px !important;
}
.upload-row{
    border: 1px solid #b3adad;
    width: 350px;
    height: 46px;
    padding: 10px;
}
#upload-btn-content {
    color: #fff;
    background-color: #767676;
    border-color: #767676;
    display: inline-block;
    padding: 6px 12px;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: 400;
    line-height: 1.42857143;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    -ms-touch-action: manipulation;
    touch-action: manipulation;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
}
button#upload-btn-button:hover {
    background: transparent !important;
}
.csfWidgets.textAreaContainer{
    max-width:100% !important;
}
.csfWidgets textarea {
    border-color: #2125294f !important;
}
strong.iconUpload:hover {
    cursor: pointer;
}
div#uploadTextArea {
    width: 100%;
    height: 300px;
    border: 1px solid #ccc;
    padding: 5px;
    overflow-y: auto;
}
.well{
	min-height: 20px;
    padding: 19px;
    margin-bottom: 20px;
    background-color: #f5f5f5;
    border: 1px solid #e3e3e3;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.05);
    box-shadow: inset 0 1px 1px rgba(0,0,0,.05);
}
.container{
	-webkit-box-shadow: 3px 9px 41px 2px rgba(0,0,0,0.56);
	-moz-box-shadow: 3px 9px 41px 2px rgba(0,0,0,0.56);
	box-shadow: 3px 9px 41px 2px rgba(0,0,0,0.56);
    border: 1px solid #ccc;
    padding: 25px;
    margin-top: 10px;
}
.center{
	text-align:center;
}